*{

    box-sizing: border-box;
    margin: 0;
    padding: 0;
    border: 0;   
}

body
{
    background: #FFFFFF;
    color: #000000;
    overflow-x: hidden;
    -o-text-size-adjust: none;
	width: 100%;
}

/* Layout classes */
.kbasemargin
{
    margin: 0px;
}
.kbasepadding
{
    padding: 0px;
}
.kheight
{
	min-height: 28px;
}
.kselect
{
