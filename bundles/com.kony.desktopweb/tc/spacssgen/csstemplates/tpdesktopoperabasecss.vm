/* Widget classes */
input[type="radio"]
{
	
	height:20px !important;
	width:20px !important;
}
input[type="checkbox"]
{
	height:24px !important;
	width:24px !important;
}
img 
{
	vertical-align: bottom;
	-o-user-drag: none;
	-o-user-select: none;
}
div[kwidgettype="Label"] 
{
	white-space:pre-wrap;
	word-wrap:break-word;
}


div[kwidgettype="Link"]:hover
{
 cursor: pointer
}

optgroup > option {
	padding-left: 10px;
}

.voltmxlink
{
    text-decoration: none;
}
.voltmxsliderleft
{
    background-color : grey;
}
.voltmxsliderright
{
    background-color : black;
}
.kstripcontainer 
{
    height: auto;
    width: 100%;
    overflow: hidden;
    position: relative;
}
.kstrip 
{
    float: left;
    display: inline;
    padding: 0px;
    margin: 0px;
  
}

/* switch css */
.switch 
{
    overflow: hidden;
    text-align: center;
    line-height: 25px;
}
.switch .thumb 
{
    position: relative;
    z-index: 3;
    border: solid 1px #919191;
}
.switch .thumb span 
{
    display: block;

    min-height: 25px;
}
.switch .on 
{
    color: #fff;
    min-height: 27px;
    border: solid 1px #093889;
    padding-right: 4px;
    margin-right: -6px;     
}
.switch .off 
{
    color: #666;
    min-height: 27px;
    border: solid 1px #a1a1a1;
    padding-left: 4px;
    margin-left: -6px;
}

/* Popup css */
.popupcontainer
{
	position:fixed !important;	
	position:absolute;
	width: 100%;
	height:100%;
	z-index: 8;
    bottom : 0;
    top : 0;
}
.popuplayer 
{
    position:fixed !important;
    position: absolute;
	width: 100%;
    height: 100%;
    background-color: black;
    opacity: 0.5;   
}
.popupcontrols
{
    position: absolute;
    right: 1px;
    top: 2px;
    cursor: pointer;
}

/* fade css */
.scroll_fades 
{
    position: absolute; 
    z-index: 100;
}
.scroll_view 
{
    position: absolute !important;
    left: 0;
    top: 0;
    right: 0;
    height: 100%;
}.scrollerX {
    overflow: hidden;
    position: relative;
}
.scrolleeX
{
    white-space: nowrap;
}
.scroll_fades {
    position: absolute;
    z-index: 100;
}
.leftfade 
{
    top: 50%;
    left: 0; 
}
.rightfade 
{
    top: 50%;
    right: 0;
}
.topfade 
{
    top: 0;
    left: 50%; 
}
.bottomfade 
{
    bottom: 0;
    left: 50%;
}


/* Transitions */
@-o-keyframes slidetopout
{
    from { -o-transform: translateY(0%); }
    to { -o-transform: translateY(-100%); }
}
@-o-keyframes slidetopin
{
	from { -o-transform: translateY(100%); }
    to { -o-transform: translateY(0); }
}

@-o-keyframes slidebottomout
{
    from { -o-transform: translateY(0%); }
    to { -o-transform: translateY(100%); }
}
@-o-keyframes slidebottomin
{
    from { -o-transform: translateY(-100%); }
    to { -o-transform: translateY(0); }
}

@-o-keyframes slideleftout
{
    from { -o-transform: translateX(0); }
    to { -o-transform: translateX(-100%); }
}
@-o-keyframes slideleftin
{
    from { -o-transform: translateX(100%); }
    to { -o-transform: translateX(0); }
}

@-o-keyframes sliderightout
{
    from { -o-transform: translateX(0); }
    to { -o-transform: translateX(100%); }
}
@-o-keyframes sliderightin
{
    from { -o-transform: translateX(-100%); }
    to { -o-transform: translateX(0); }
}

@-o-keyframes fadeout
{
    from { opacity: 1; }
    to { opacity: 0; }
}
@-o-keyframes fadein
{
    from { opacity: 0; }
    to { opacity: 1; }
}


