a[selected="progress"] 
{
    background-image: url(./images/loading.gif), url(./images/selection.png) !important; 
    background-repeat: no-repeat;
    background-position: center center;
    background-color:  transparent;
}

div[selected="progress"]
{
    background-image: url(./images/loading.gif) !important;
    background-repeat: no-repeat;
    background-color:  transparent;
    z-index: 998;
    width:25px;
    height:20px;
    position: absolute;
}
    
#footer 
{
    position: absolute;
    top: 358px;
    height: 60px;
    background-position: center bottom;
    width:100%;
    background:none;
}

#calendarcontainer {
        padding:10px;
}

#calendarmenu{
    position: relative;
}

#calendarpicker button{
        background: url(../tparty/cal/calendar_icon.gif) center center no-repeat;
        text-align: left;
        text-indent: -10em;
        overflow: hidden;
        white-space: normal;
}

.panelcollapsed .panelcontent { display: none; }

.panelcontent
{
	overflow: hidden;
}

.inactive {
    cursor:default;
    background-position:0 -30px !important;
}

.previousArrowNav {
    background:url("./images/previous.png") no-repeat scroll 0 0 transparent;
}

body > .popup, .dCover {
    min-height:0px;
    display:block;
}

.popup{
    position: absolute;
    z-index: 798;
    background:transparent;
}
.popuptitle {
    font-family:sans-serif;
    font-size:12px;
    float:left;
    color:blue;
}
.close{
    float:right;
}

.popupbody{
    float:left;
}
.popuptitleBar{
    float:left;
    border-bottom:1px solid #999999;
    width:100%
}

.cover {
    background:transparent;
    height:100%;
    left:0;
    position:absolute;
    top:0;
    vertical-align:middle;
    width:100%;
}


.nextArrowNav {
    background:url("./images/next.png") no-repeat scroll 0 0 transparent;
}
.previousArrowNav, .nextArrowNav {
    height:30px;
    width:30px;
    overflow:hidden;
    text-indent:-9999px;
    display:block;
    cursor:pointer;
    margin-left:10px
}

