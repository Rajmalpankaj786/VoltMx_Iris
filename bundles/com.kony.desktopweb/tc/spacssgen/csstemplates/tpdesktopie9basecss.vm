/* switch css */
.switch
{
	display: box;
	border-radius: 6px;
	user-select: none;
	overflow: hidden;
	text-align: center;
	line-height: 25px;
}
.switch .thumb
{
	position: relative;
	border-radius: 7px;
	z-index: 3;
	border: solid 1px #919191;
}
.switch .thumb span
{
	display: block;
	border-radius: 6px;
	background-image: gradient(linear, left top, left bottom, from(#cecece), to(#fbfbfb));
	min-height: 25px;
}
.switch .on
{
	color: #fff;
	min-height: 27px;
	background-image: gradient(linear, left top, left bottom, from(#295ab2), to(#76adfc));
	border-top-left-radius: 6px;
	border-bottom-left-radius: 6px;
	border: solid 1px #093889;
	padding-right: 4px;
	margin-right: -6px;
}
.switch .off
{
	color: #666;
	min-height: 27px;
	background-image: gradient(linear, left top, left bottom, from(#b5b5b5), color-stop(0.50, #fff));
	border: solid 1px #a1a1a1;
	padding-left: 4px;
	margin-left: -6px;
}

.switchSkinon 
{
                color: #fff;
                min-height: 27px;
    filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0, startColorstr='#295ab2', endColorstr='#76adfc'); 
                border-top-left-radius: 6px;
                border-bottom-left-radius: 6px;
                border: solid 1px #093889;
                padding-right: 4px;
                margin-right: -6px;
}
.switch .switchThumb
{
                display: block;
                border-radius: 6px;
                filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0, startColorstr='#cecece', endColorstr='#fbfbfb');  
                min-height: 27px;
                width : 50%;
                height:29px;
                position:absolute;
                top:1px;
}


