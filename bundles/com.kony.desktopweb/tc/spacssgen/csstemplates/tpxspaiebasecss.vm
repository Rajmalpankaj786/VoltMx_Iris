/*
 * Pull down styles
 */

div[name="pullDown"], div[name="pullUp"] 
{
	background:#fff;
	height:40px;
	line-height:40px;
	padding:5px 10px;
	/*border-bottom:1px solid #ccc;*/
	font-weight:bold;
	font-size:14px;
	color:#888;
	
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
div[name="pullDown"] .pullDownIcon, div[name="pullUp"] .pullUpIcon  
{
	display:block; float:left;
	width:40px; height:40px;
	background:url("./images/pull-icon.png") 0 0 no-repeat;
	-ms-background-size:40px 80px; 
	background-size:40px 80px;
	-ms-transition-property:-webkit-transform;
	-ms-transition-duration:250ms;	
}
div[name="pullDown"] .pullDownIcon 
{
	-ms-transform:rotate(0deg) translateZ(0);
}
div[name="pullUp"] .pullUpIcon  
{
	-ms-transform:rotate(-180deg) translateZ(0);
}
div[name="pullDown"].flip .pullDownIcon 
{
	-ms-transform:rotate(-180deg) translateZ(0);
}
div[name="pullUp"].flip .pullUpIcon {
	-ms-transform:rotate(0deg) translateZ(0);
}
div[name="pullDown"].loading .pullDownIcon, div[name="pullUp"].loading .pullUpIcon 
{
	background-position:0 100%;
	-ms-transform:rotate(0deg) translateZ(0);
	-ms-transition-duration:0ms;

	-ms-animation-name:loading;
	-ms-animation-duration:2s;
	-ms-animation-iteration-count:infinite;
	-ms-animation-timing-function:linear;
}
@-ms-keyframes loading 
{
	from { -ms-transform:rotate(0deg) translateZ(0); }
	to { -ms-transform:rotate(360deg) translateZ(0); }
}

