.container-horiz {
    display: flex;
    align-items: flex-start;
    flex-wrap: nowrap;
}
.container-horiz > * {
    flex-shrink: 0;
    overflow-wrap: break-word;
    position: relative;
}
.container-horiz-reverse {
    flex-direction: row-reverse;
}
.container-freeform {
    position: relative;
}
.container-freeform > * {
    position: absolute;
}
.container-vert {
    display: flex;
    align-items: flex-start;
    flex-flow: column nowrap;
}
.container-vert > * {
    flex-shrink: 0;
    overflow-wrap: break-word;
    position: relative;
}
.container-vert-reverse {
    flex-direction: column-reverse;
}
