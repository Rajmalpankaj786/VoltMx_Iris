/* switch css */
.switch
{
        display: -moz-box;
        display: -ms-flexbox;
        display: box;
	display: -webkit-box;
        -moz-border-radius: 6px;
	-webkit-border-radius: 6px;
        border-radius: 6px;
        -moz-user-select: none;
        -ms-user-select: none;
	-webkit-user-select: none;
        user-select: none;
	overflow: hidden;
	text-align: center;
	line-height: 25px;
}
.switch .thumb
{
	position: relative;
        -moz-border-radius:7px;
	-webkit-border-radius: 7px;
        border-radius: 7px;
	z-index: 3;
	border: 1px solid #919191;
}
.switch .thumb span
{
	display: block;
	-webkit-border-radius: 6px;
        border-radius: 6px;
        background : -moz-linear-gradient(46% 0% 270deg, #cecece, #fbfbfb);
        background-image: gradient(linear, left top, left bottom, from(#cecece), to(#fbfbfb));
        background-image: -ms-linear-gradient(top, #cecece, #fbfbfb);
	background-image: -webkit-gradient(linear, left top, left bottom, from(#cecece), to(#fbfbfb));
	background-image: -webkit-linear-gradient(top, #cecece, #fbfbfb);
	min-height: 25px;
}
.switch .on
{
	color: #fff;
	min-height: 27px;
         filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0, startColorstr='#295ab2', endColorstr='#76adfc');
        background : -moz-linear-gradient(46% 0% 270deg, #295ab2, #76adfc);
	background-image: -webkit-gradient(linear, left top, left bottom, from(#295ab2), to(#76adfc));
	background-image: -webkit-linear-gradient(top, #295ab2, #76adfc);
        -moz-border-top-left-radius: 6px;
        -ms-border-top-left-radius: 6px;
	-webkit-border-top-left-radius: 6px;
        border-top-left-radius: 6px;
        -moz-border-bottom-left-radius: 6px;
        -ms-border-bottom-left-radius: 6px;
	-webkit-border-bottom-left-radius: 6px;
        border-bottom-left-radius: 6px;
	border: 1px solid #093889;
	//padding-right: 4px;
	margin-right: -6px;
}
.switch .off
{
	color: #666;
	min-height: 27px;
        background : -moz-linear-gradient(46% 0% 270deg, #b5b5b5, #ffffff);
	background-image: -webkit-gradient(linear, left top, left bottom, from(#b5b5b5), color-stop(0.50, #fff));
	background-image: -webkit-linear-gradient(top, #b5b5b5 0%, #fff 50%);
        -moz-border-top-right-radius: 6px;
        -ms-border-top-right-radius: 6px;
	-webkit-border-top-right-radius: 6px;
        border-top-right-radius: 6px;
        -moz-border-bottom-right-radius: 6px;
        -ms-border-bottom-right-radius: 6px;
	-webkit-border-bottom-right-radius: 6px;
        border-bottom-right-radius: 6px;
	border: 1px solid #a1a1a1;
	//padding-left: 4px;
	margin-left: -6px;
}

