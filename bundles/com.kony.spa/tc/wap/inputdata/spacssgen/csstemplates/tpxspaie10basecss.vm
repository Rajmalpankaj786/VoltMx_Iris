/* switch css */
.switch
{
	display: -ms-flexbox;
    border-radius: 6px;
	-ms-user-select: none;
	overflow: hidden;
	text-align: center;
	line-height: 25px;
}
.switch .thumb
{
	position: relative;
    border-radius: 7px;
	z-index: 3;
	border: 1px solid #919191;
}
.switch .thumb span
{
	display: block;
    border-radius: 6px;
	background-image: -ms-linear-gradient(top, #cecece, #fbfbfb);
	min-height: 25px;
}
.switch .on
{
	color: #fff;
	min-height: 27px;
	background-image: -ms-linear-gradient(top, #295ab2, #76adfc);
	-ms-border-top-left-radius: 6px;
    border-top-left-radius: 6px;
	-ms-border-bottom-left-radius: 6px;
    border-bottom-left-radius: 6px;
	border: 1px solid #093889;
	padding-right: 4px;
	margin-right: -6px;
}
.switch .off
{
	color: #666;
	min-height: 27px;
	background-image: -ms-linear-gradient(top, #b5b5b5 0%, #fff 50%);
	-ms-border-top-right-radius: 6px;
    border-top-right-radius: 6px;
	-ms-border-bottom-right-radius: 6px;
    border-bottom-right-radius: 6px;
	border: 1px solid #a1a1a1;
	padding-left: 4px;
	margin-left: -6px;
}


