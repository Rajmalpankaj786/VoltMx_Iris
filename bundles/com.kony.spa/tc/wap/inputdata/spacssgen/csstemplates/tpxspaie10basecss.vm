/* Transitions */
@-ms-keyframes slidetopout
{
    from { -ms-transform: translateY(0%); }
    to { -ms-transform: translateY(-100%); }
}
@-ms-keyframes slidetopin
{
	from { -ms-transform: translateY(100%); }
    to { -ms-transform: translateY(0); }
}
@-ms-keyframes slidebottomout
{
    from { -ms-transform: translateY(0%); }
    to { -ms-transform: translateY(100%); }
}
@-ms-keyframes slidebottomin
{
    from { -ms-transform: translateY(-100%); }
    to { -ms-transform: translateY(0); }
}
@-ms-keyframes slideleftout
{
    from { -ms-transform: translateX(0); }
    to { -ms-transform: translateX(-100%); }
}
@-ms-keyframes slideleftin
{
    from { -ms-transform: translateX(100%); }
    to { -ms-transform: translateX(0); }
}
@-ms-keyframes sliderightout
{
    from { -ms-transform: translateX(0); }
    to { -ms-transform: translateX(100%); }
}
@-ms-keyframes sliderightin
{
    from { -ms-transform: translateX(-100%); }
    to { -ms-transform: translateX(0); }
}
@-ms-keyframes fadeout
{
    from { opacity: 1; }
    to { opacity: 0; }
}
@-ms-keyframes fadein
{
    from { opacity: 0; }
    to { opacity: 1; }
}


