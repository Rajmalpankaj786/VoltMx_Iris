.nextArrowNav {
#if($bjsVersion)
#set($bjsversn = "./images/"+ $bjsVersion+"/next.png")
background:url("$bjsversn") no-repeat scroll 0 0 transparent;
#else
background:url("./images/next.png") no-repeat scroll 0 0 transparent;
#end
}
.previousArrowNav, .nextArrowNav {
    height:30px;
    width:30px;
    overflow:hidden;
    text-indent:-9999px;
    display:block;
    cursor:pointer;
    margin-left:10px
}

.inactive {
    cursor:default;
    background-position:0 -30px !important;
}

.previousArrowNav {
#if($bjsVersion)
#set($bjsversn = "./images/"+ $bjsVersion+"/previous.png")
background:url("$bjsversn") no-repeat scroll 0 0 transparent;
#else
background:url("./images/previous.png") no-repeat scroll 0 0 transparent;
#end
}

